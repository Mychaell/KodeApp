<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DA7051"
    tools:context=".secondact">

    <TextView
        android:id="@+id/firstTextTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WELCOME \n BACK \n HERE"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="26sp"
        android:textStyle="bold"
        android:layout_marginTop="50dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        />

    <ImageView
        android:id="@+id/imageTwo"
        android:layout_width="150dp"
        android:layout_height="200dp"
        android:src="@drawable/nft"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/imageTwotwo"



        />

    <ImageView
        android:id="@+id/imageTwotwo"
        android:layout_width="150dp"
        android:layout_height="200dp"
        android:src="@drawable/nft"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@id/imageTwo"


        />

    <TextView
        android:id="@+id/secondTextTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="This has to be the best place to be. But e choke sha, no reason am. ZZzzzzzzzzzzznehbcjA"
        android:textSize="24sp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"


        />

</androidx.constraintlayout.widget.ConstraintLayout>